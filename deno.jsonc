{
  "tasks": {
    "dev-old": "deno run --allow-net --allow-read --allow-env=NODE_DEBUG --watch main.ts --dev",
    "serve-old": "deno run --allow-net --allow-read --no-check main.ts",
    "build-astro-deno": "deno run --unstable --allow-env=NODE_DEBUG npm:astro@^1.1.5 build",
    "build": "npm run build",
    "preview": "npm run build && deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs"
  }
}